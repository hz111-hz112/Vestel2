<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_yemek_verisi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="160dp"
        android:layout_marginBottom="0dp"
        android:onClick="yenisayfa"
        android:src="@drawable/yemekk"
        android:visibility="invisible"
        app:backgroundTint="#ffffff"
        app:fabSize="normal"

        />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_oneri"
        android:layout_width="wrap_content"
        android:layout_height="63dp"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="125dp"
        android:layout_marginBottom="55dp"
        android:src="@drawable/oneri"
        app:backgroundTint="#ffffff"
        app:fabSize="normal"
        android:visibility="invisible"
        />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_skt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="76dp"
        android:layout_marginBottom="96dp"
        android:src="@drawable/skt"
        android:visibility="invisible"
        app:backgroundTint="#ffffff"
        app:fabSize="normal" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_miktar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="130dp"
        android:src="@drawable/miktar"
        app:backgroundTint="#ffffff"
        app:fabSize="normal"
        android:visibility="invisible"/>


</FrameLayout>